
{
  "name": "tree",
  "version": 2,
  "public": true,
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    { "src": "^/(.*)", "dest": "/index.html" }
  ],
  "github": {
    "autoAlias": true,
    "silent": true
  }
}
